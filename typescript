/*Written by Lorrayya Williams
Adds all items in a list to the ostream and returns that stream<<*/
template <class Item>
ostream& operator<<(ostream& out, const List<Item>& alist){
	alist.writeTo(out, '\t');
	return out;
}

#endif /* LIST_H_ */
